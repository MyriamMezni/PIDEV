




{% extends "Front.html.twig" %}

{% block content %}




    <div class="site-section py-5 bg-warning">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12 d-flex">
                    <h2 class="text-white m-0">Voir les statistiques des activités</h2>
                    <a href="{{ path('chart') }}" class="btn btn-primary btn-custom-1 py-3 px-5 ml-auto">Cliquer içi</a>
